<!doctype html>
<html lang="en">
    <head>
        <!-- Required meta tags -->
        <meta charset="utf-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta name="description" content="{% block description %}En Güncel İlanlar!{% endblock %}">
        <meta name="keywords" content="{% block keywords %} ilan,etkinlik{% endblock %}">
        <meta name="author" content="Merve Bozkurt">

        {% block head %}{% include ('home/head.html.twig') %}{% endblock %}
        {% block stylesheet %}{% endblock %}

    </head>
    <body>
        {% block header %}{% include ('home/header.html.twig') %}{% endblock %}
        {% block sidebar %}{% endblock %}
        {% block slider %} {% endblock %}
        {% block body %}{% endblock %}
        {% block footer %}{% include ('home/footer.html.twig') %}{% endblock %}
        {% block javascript %}{% include ('home/javascript.html.twig') %}{% endblock %}

    </body>
</html>
